<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
   <welcome-file-list>
    <welcome-file>webapps?r=igrp/home/index</welcome-file>
  </welcome-file-list>
  
  <display-name>IGRP</display-name>
  
	 <servlet>
	 	<servlet-name>igrp</servlet-name>
	 	<servlet-class>nosi.core.servlet.IgrpServlet</servlet-class>
	 </servlet>
 
	 <servlet-mapping>
	 	<servlet-name>igrp</servlet-name>
	 	<url-pattern>/</url-pattern>
	 </servlet-mapping>
	 
	 <servlet-mapping>
	    <servlet-name>default</servlet-name>
	    <url-pattern>/images/IGRP/IGRP2.3/*</url-pattern>
	    <url-pattern>/images/IGRP/IGRP2.3/core/formgen/util/servlet/*</url-pattern>
	    <url-pattern>/images/IGRP/Config/*</url-pattern>
	</servlet-mapping>

