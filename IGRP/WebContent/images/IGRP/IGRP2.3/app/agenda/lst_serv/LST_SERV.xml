        <with_label>false</with_label>
        <with_biztalk>false</with_biztalk>
        <dynamic_menu>false</dynamic_menu>
        <copy_menu>false</copy_menu>
        <package_copy_db />
        <package_copy_html />
        <preserve_url>http://localhost:8080/IGRP/webapps?r=igrp/page/preserveUrl&amp;p_id=</preserve_url>
    </plsql>
    <content>
        <title>Lista de Serviços</title>
        <form_1 type="form" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <entidade name="p_entidade" type="text" maxlength="30" required="false" change="false" readonly="true" disabled="false" placeholder="" right="false">
                    <label>Entidade</label>
                    <value>Sit perspiciatis officia perspiciatis deserunt</value>
                </entidade>
            </fields>
            <tools-bar>
                <item type="form" code="" class="success" rel="novo">
                    <title>Novo</title>
                    <app>agenda</app>
                    <page>AddServicos</page>
                    <link>index</link>
                    <parameter/>
                    <target>modal</target>
                    <img>success|fa-plus-square</img>
                </item>
                <item type="form" code="" class="warning" rel="editar">
                    <title>Editar</title>
                    <app>agenda</app>
                    <page>AddServicos</page>
                    <link>index</link>
                    <parameter/>
                    <target>modal</target>
                    <img>warning|fa-edit</img>
                </item>
                <item type="form" code="" class="danger" rel="remover">
                    <title>Remover</title>
                    <app>agenda</app>
                    <page>AddServicos</page>
                    <link>index</link>
                    <parameter/>
                    <target>alert_submit</target>
                    <img>danger|fa-close</img>
                </item>
            </tools-bar>
        </form_1>
        <table_1 type="table" xml-type="table" gen-type="container" gen-group="true">
            <fields>
                <servico name="p_servico" type="text" maxlength="30" align="left" lookup_parser="false">
                    <label>Serviço</label>
                </servico>
                <checkbox_1 name="p_checkbox_1" type="checkbox" maxlength="30" align="left" lookup_parser="false" check="true" desc="true">
                    <label>Checkbox</label>
                </checkbox_1>
                <filter name="p_table_1_filter" type="filter_az">
                    <value/>
                </filter>
            </fields>
            <table>
                <value>
                    <row>
                        <servico name="p_servico">Perspiciatis aliqua aperiam dolor adipiscing</servico>
                        <checkbox_1 name="p_checkbox_1" check="true" desc="true">1</checkbox_1>
                        <checkbox_1_desc name="p_checkbox_1_desc">1</checkbox_1_desc>
                    </row>
                    <row>
                        <servico name="p_servico">Stract deserunt aperiam elit laudantium</servico>
