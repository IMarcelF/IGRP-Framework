<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet href="images/IGRP/IGRP2.3/app/agenda/addservicos/AddServicos.xsl" type="text/xsl"?>
<rows>
    <site>
        <welcome_note>Ola</welcome_note>
        <footer_note>2011 - Copyright NOSI</footer_note>
        <user_name>red-igrp</user_name>
        <button type="specific">
            <title>Sair</title>
            <app>RED</app>
            <page />
            <link>#</link>
            <target>_self</target>
            <img>exit.png</img>
        </button>
    </site>
    <page>AddServicos</page>
    <app>agenda</app>
    <plsql>
        <action>index</action>
        <package_instance></package_instance>
        <package_db>nosi.webapps.agenda.pages</package_db>
        <package_html>AddServicos</package_html>
        <table_name></table_name>
        <with_replace>false</with_replace>
        <with_label>false</with_label>
        <with_biztalk>false</with_biztalk>
        <dynamic_menu>false</dynamic_menu>
        <copy_menu>false</copy_menu>
        <package_copy_db />
        <package_copy_html />
        <preserve_url>http://localhost:8080/IGRP/webapps?r=igrp/page/preserveUrl&amp;p_id=</preserve_url>
    </plsql>
    <content>
        <title>Adicionar Serviços</title>
        <box_1 type="box" xml-type="form" gen-type="container" gen-group="">
            <fields/>
        </box_1>
        <page_title type="sectionheader" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <page_title_text type="text" name="p_page_title_text" persist="true" maxlength="4000">
                    <value>
                        <![CDATA[Associar/Desassociar Serviços]]>
                    </value>
                </page_title_text>
            </fields>
        </page_title>
        <form_1 type="form" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <entidade name="p_entidade" type="select" multiple="false" maxlength="30" required="true" change="true" disabled="false" right="false">
                    <label>Entidade</label>
                    <list>
                        <option>
                            <text>Option 1</text>
                            <value>1</value>
                        </option>
                        <option>
                            <text>Option 2</text>
                            <value>2</value>
                        </option>
                        <option>
                            <text>Option 3</text>
                            <value>3</value>
                        </option>
                        <option>
                            <text>Option 4</text>
                            <value>4</value>
                        </option>
                    </list>
                </entidade>
                <balcao name="p_balcao" type="select" multiple="false" maxlength="30" required="true" change="false" disabled="false" right="false">
                    <label>Balcão</label>
                    <list>
                        <option>
                            <text>Option 1</text>
                            <value>1</value>
                        </option>
                        <option>
                            <text>Option 2</text>
                            <value>2</value>
                        </option>
                        <option>
                            <text>Option 3</text>
                            <value>3</value>
                        </option>
                        <option>
                            <text>Option 4</text>
                            <value>4</value>
                        </option>
                    </list>
                </balcao>
            </fields>
            <tools-bar>
                <item type="form" code="" class="info" rel="filtrar">
                    <title>Filtrar</title>
                    <app>agenda</app>
                    <page>AddServicos</page>
                    <link>index</link>
                    <parameter/>
                    <target>_self</target>
                    <img>info|fa-filter</img>
                </item>
            </tools-bar>
        </form_1>
        <table_1 type="table" xml-type="table" gen-type="container" gen-group="true">
            <fields>
                <servicos name="p_servicos" type="text" maxlength="30" align="left" lookup_parser="false">
                    <label>Serviços</label>
                </servicos>
                <checkbox_1 name="p_checkbox_1" type="checkbox" maxlength="30" align="left" lookup_parser="false" check="true" desc="true">
                    <label>Checkbox</label>
                </checkbox_1>
            </fields>
            <table>
                <value>
                    <row>
                        <servicos name="p_servicos">Accusantium unde rem adipiscing officia</servicos>
                        <checkbox_1 name="p_checkbox_1" check="true" desc="true">1</checkbox_1>
                        <checkbox_1_desc name="p_checkbox_1_desc">1</checkbox_1_desc>
                    </row>
                    <row>
                        <servicos name="p_servicos">Aperiam amet adipiscing sit deserunt</servicos>
                        <checkbox_1 name="p_checkbox_1" check="true" desc="true">1</checkbox_1>
                        <checkbox_1_desc name="p_checkbox_1_desc">1</checkbox_1_desc>
                    </row>
                    <row>
                        <servicos name="p_servicos">Natus perspiciatis dolor lorem sit</servicos>
                        <checkbox_1 name="p_checkbox_1" check="true" desc="true">1</checkbox_1>
                        <checkbox_1_desc name="p_checkbox_1_desc">1</checkbox_1_desc>
                    </row>
                    <row>
                        <servicos name="p_servicos">Omnis labore aperiam laudantium natus</servicos>
                        <checkbox_1 name="p_checkbox_1" check="true" desc="true">1</checkbox_1>
                        <checkbox_1_desc name="p_checkbox_1_desc">1</checkbox_1_desc>
                    </row>
                </value>
            </table>
        </table_1>
        <toolsbar_1 type="toolsbar" xml-type="toolsbar" gen-type="menu" gen-group="">
            <item type="specific" code="" rel="gravar">
                <title>Gravar</title>
                <app>agenda</app>
                <page>AddServicos</page>
                <link>index</link>
                <target>submit</target>
                <img>success|fa-save</img>
                <parameter>http://igrp.teste.gov.cv/images/IGRP/images/IGRP/IGRP2.3/app/agenda/addservicos/AddServicos.xsl</parameter>
                <params>success||fa-save|www</params>
            </item>
        </toolsbar_1>
    </content>
</rows>
